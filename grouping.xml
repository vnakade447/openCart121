    <classes>
      <!--  <class name="testCases.TC003_LoginDDT"/>-->
      <class name="testCases.TC001_AccountRegistrationTest"/>
      <class name="testCases.TC002_LoginTest"/>
    </classes>
  </test> 
  
</suite> <!-- Suite -->
